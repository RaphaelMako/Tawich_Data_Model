tawich:InterviewTimestamp a rdfs:Property ;
    rdfs:label "Interview Timestamp"@en ;
    rdfs:domain tawich:Quote ;
    rdfs:range rdfs:Literal .

#tawich:Birthsite a rdfs:Property ;
