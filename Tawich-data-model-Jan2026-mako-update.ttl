# --- Classes ---
######################################

tawich:SpeciesObservation a rdfs:Class ;
    rdfs:label "Species Observation"@en ;
    rdfs:comment "Observation of an instance of a species, this will connect to geo-package's geo data." ;
    rdfs:subClassOf geo:Feature .


#tawich:KnowledgeHolderName a rdfs:Class ;
#    rdfs:label "Knowledge Holder Name"@en ;
#    rdfs:comment "Name of the knowledge holder who provided the interview." .

tawich:SubRegion a rdfs:Class ;
    rdfs:label "Sub-Region"@en ;
    rdfs:comment "Firelight sentiment analysis category." ;
    rdfs:subClassOf tawich:Location .

tawich:GatheringPlace a rdfs:Class ;
    rdfs:label "Gathering Place"@en ;
    rdfs:subClassOf tawich:Site .

tawich:BirthPlace a rdfs:Class ;
    rdfs:label "Birth Place"@en ;
    rdfs:subClassOf tawich:Site .



